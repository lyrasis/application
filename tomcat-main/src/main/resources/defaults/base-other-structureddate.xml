<record id="structureddate" in-recordlist="no" separate-record="false" services-type="structuredDateGroup">
	<section id="structuredDateInformation">
		<field id="dateNote" />
		<field id="dateAssociation" />
		<field id="datePeriod" />
		<field id="dateDisplayDate" />

		<field id="dateEarliestSingleYear" datatype="integer" />
		<field id="dateEarliestSingleMonth" datatype="integer" />
		<field id="dateEarliestSingleDay" datatype="integer" />
		<field id="dateEarliestSingleEra" autocomplete="true" ui-type="enum" />

		<field id="dateEarliestSingleCertainty" autocomplete="true" ui-type="enum" />
		<field id="dateEarliestSingleQualifier" />
		<field id="dateEarliestSingleQualifierValue" datatype="integer" />
		<field id="dateEarliestSingleQualifierUnit" autocomplete="true" ui-type="enum" />

		<field id="dateLatestYear" datatype="integer" />
		<field id="dateLatestMonth" datatype="integer" />
		<field id="dateLatestDay" datatype="integer" />
		<field id="dateLatestEra" autocomplete="true" ui-type="enum" />

		<field id="dateLatestCertainty" autocomplete="true" ui-type="enum" />
		<field id="dateLatestQualifier" />
		<field id="dateLatestQualifierValue" datatype="integer" />
		<field id="dateLatestQualifierUnit" autocomplete="true" ui-type="enum" />

		<field id="dateEarliestScalarValue" datatype="date" />
		<field id="dateLatestScalarValue" datatype="date" />
		<field id="scalarValuesComputed" datatype="boolean" />
	</section>
</record>
