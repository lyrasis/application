<record id="collection-object">
	<services-record-path id="nagpra">collectionobjects_nagpra:http://collectionspace.org/services/collectionobject/domain/nagpra,collectionobjects_nagpra</services-record-path>

	<section id="nagpraInformation">
		<repeat id="nagpraInventoryNames" section="nagpra">
			<field id="nagpraInventoryName" section="nagpra" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="nagpraCategories" section="nagpra">
			<field id="nagpraCategory" section="nagpra" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="graveAssocCodes" section="nagpra">
			<field id="graveAssocCode" section="nagpra" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="nagpraCulturalDeterminations" section="nagpra">
			<field id="nagpraCulturalDetermination" section="nagpra" />
		</repeat>

		<repeat id="repatriationNotes" section="nagpra">
			<field id="repatriationNote" section="nagpra" />
		</repeat>

		<field id="nagpraReportFiled" section="nagpra" datatype="boolean" />
		<field id="nagpraReportFiledBy" section="nagpra" autocomplete="true" />
		<field id="nagpraReportFiledDate" section="nagpra" ui-type="groupfield/structureddate" />
	</section>
</record>
