<record id="loanin" is-extension="true">
	<services-record-path id="naturalhistory_extension">loansin_naturalhistory_extension:http://collectionspace.org/services/loanin/domain/naturalhistory_extension,loansin_naturalhistory_extension</services-record-path>

	<section id="returnInformation">
		<repeat id="returnGroupList/returnGroup" section="naturalhistory_extension">
			<field id="returnObjectNumbers" section="naturalhistory_extension" />
			<field id="returnQuantity" section="naturalhistory_extension" datatype="integer" />
			<field id="returnDate" section="naturalhistory_extension" datatype="date" />
			<field id="returnNotes" section="naturalhistory_extension" />
			<field id="returnDetermination" section="naturalhistory_extension" autocomplete="true" />
		</repeat>
	</section>

	<section id="transferOutInformation">
		<repeat id="transferOutGroupList/transferOutGroup" section="naturalhistory_extension">
			<field id="transferOutDate" section="naturalhistory_extension" datatype="date" />
			<field id="transferOutQuantity" section="naturalhistory_extension" datatype="integer" />
			<field id="transferOutOrg" section="naturalhistory_extension" autocomplete="true" />
			<field id="transferOutPerson" section="naturalhistory_extension" autocomplete="true" />
			<field id="transferOutDirector" section="naturalhistory_extension" autocomplete="true" />
			<field id="transferOutNotes" section="naturalhistory_extension" />
			<field id="transferOutReturnDate" section="naturalhistory_extension" datatype="date" />
			<field id="transferOutReturnQuantity" section="naturalhistory_extension" datatype="integer" />
			<field id="transferOutReturnNotes" section="naturalhistory_extension" />
		</repeat>
	</section>
</record>
