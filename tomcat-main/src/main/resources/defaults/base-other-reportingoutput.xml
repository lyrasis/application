<record id="output" in-recordlist="no" type="authorizationdata,report">
	<web-url>invokereport</web-url>
	<services-url>reports</services-url>
	<services-record-path>invocationContext:http://collectionspace.org/services/common/invocable,invocationContext</services-record-path>

	<authorization-view>false</authorization-view>
	<terms-used>false</terms-used>
	<refobj-used>false</refobj-used>
	<structures>
		<structure id="screen">

		</structure>
	</structures>

	<include src="domain-other-reportingoutput.xml" strip-root="yes" />

	<section id="reportingOutputInformation">
		<field id="docType" />
		<field id="mode" />
		<field id="singleCSID" />
		<field id="groupCSID" />
		<repeat id="listCSIDs">
			<field id="csid" />
		</repeat>
	</section>
</record>
