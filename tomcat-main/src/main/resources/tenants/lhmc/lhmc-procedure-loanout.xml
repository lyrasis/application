<record id="loanout" is-extension="true">
    <services-record-path id="lhmc">loansout_lhmc:http://collectionspace.org/services/loanout/domain/lhmc,loansout_lhmc</services-record-path>
    <section id="loanoutInformation">
    	<repeat id="loanoutStatusLHMCGroupList/loanoutStatusLHMCGroup" section="lhmc">
            <field id="loanoutStatusLHMC" ui-search="repeatable" autocomplete="vocab-lostatus" ui-type="enum" section="lhmc" />
            <field id="loanoutStatusDateLHMC" ui-type="date" datatype="date" section="lhmc" />
            <field id="loanoutStatusNoteLHMC" section="lhmc" />
            <field id="loanoutStatusAuthorizerLHMC" autocomplete="person-person,person-person_shared,person-ulan_pa,organization-organization,organization-organization_shared" section="lhmc" />
        </repeat>
    </section>
</record>