<record id="collection-object" is-extension="true">
    <services-record-path id="lhmc">collectionobjects_lhmc:http://collectionspace.org/services/collectionobject/domain/lhmc,collectionobjects_lhmc</services-record-path>
    <section id="identificationInformation">
        <repeat id="otherNumberList/otherNumber">
            <field id="numberType" autocomplete="vocab-numbertype" ui-type="enum">
                <options/>
            </field>
        </repeat>
    </section>
    <section id="descriptionInformation">
        <repeat id="objectStatusList" services-type-anonymous="false">
            <field id="objectStatus" ui-search="repeatable" seperate_ui_container="true">
                <options>
                    <option id="">Please select a value</option>
                    <option id="copy">copy</option>
                    <option id="forgery">forgery</option>
                    <option id="holotype">holotype</option>
                    <option id="original">original</option>
                    <option id="paralectotype">paralectotype</option>
                    <option id="paratype">paratype</option>
                    <option id="type">type</option>
                </options>
            </field>
        </repeat>
        <section id="contentInformation">
            <repeat id="contentPlaces">
                <field id="contentPlace" autocomplete="place-place,place-tgn_place" />
            </repeat>
        </section>
        <section id="textualInscriptionInformation">
            <repeat id="textualInscriptionGroupList/textualInscriptionGroup">
                <field id="inscriptionContentType" seperate_ui_container="true">
                    <options>
                        <option id="">Please select a value</option>
                        <option id="hallmark">Hallmark</option>
                        <option id="stamp">Stamp</option>
                        <option id="signature">Signature</option>
                        <option id="maker's-mark">Maker's Mark</option>
                        <option id="paper-tag">Paper Tag</option>
                        <option id="cloth-label">Cloth Label</option>
                    </options>
                </field>
                <field id="inscriptionContentMethod">
                    <options>
                        <option id="">Please select a value</option>
                        <option id="engraved">Engraved</option>
                        <option id="painted">Painted</option>
                        <option id="stenciled">Stenciled</option>
                        <option id="etched">Etched</option>
                        <option id="embossed">Embossed</option>
                        <option id="stamped">Stamped</option>
                        <option id="embroidered">Embroidered</option>
                        <option id="pen-and-ink">Pen &amp; Ink</option>
                        <option id="pencil">Pencil</option>
                    </options>
                </field>
            </repeat>
        </section>
    </section>
    <section id="objectProductionInformation">
        <repeat id="objectProductionPlaceGroupList/objectProductionPlaceGroup">
            <field id="objectProductionPlace" autocomplete="place-place,place-tgn_place" ui-search="repeatable" />
        </repeat>
    </section>
    <section id="objectHistoryAssociationInformation">
        <field id="ownershipPlace" autocomplete="place-place,place-tgn_place" />
        <section id="associationInformation">
            <repeat id="assocPlaceGroupList/assocPlaceGroup">
                <field id="assocPlace" autocomplete="place-place,place-tgn_place" />
            </repeat>
        </section>
    </section>
</record>
