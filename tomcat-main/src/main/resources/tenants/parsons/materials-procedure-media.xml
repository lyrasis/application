<record id="media" is-extension="true">
	<services-record-path id="materials">media_materials:http://collectionspace.org/services/media/local/materials,media_materials</services-record-path>

	<section id="mediaInformation">
		<!-- Add autocomplete -->
		<field id="coverage" autocomplete="true" />

		<!-- Local fields -->

		<repeat id="publishToList" services-type-anonymous="false" section="materials">
			<field id="publishTo" autocomplete="true" ui-type="enum" section="materials" />
		</repeat>
	</section>
</record>
