<record id="loanout">
	<services-record-path id="herbarium">loansout_herbarium:http://collectionspace.org/services/loanout/local/herbarium,loansout_herbarium</services-record-path>
	
	<section id="loanoutInformation">
		<field id="majorGroup" section="herbarium" autocomplete="vocab-majortaxongroup" ui-type="enum"/>
		
		<repeat id="geographyList" section="herbarium">
			<field id="geography" section="herbarium"/>
		</repeat>
		
		<repeat id="taxonomyList" section="herbarium">
			<field id="taxonomy" section="herbarium" autocomplete="taxon-taxon"/>
		</repeat>
		
		<repeat id="objKindList" section="herbarium">
			<field id="objKind" section="herbarium" autocomplete="vocab-objectkind" ui-type="enum"/>
		</repeat>
		
		<field id="shipDate" section="herbarium" ui-type="date" datatype="date"/>
		<field id="transportMethod" section="herbarium" autocomplete="vocab-transportmethod" ui-type="enum"/>
		<field id="numPackages" section="herbarium" ui-type="validated" datatype="integer"/>
		<field id="acknowledgeDate" section="herbarium" ui-type="date" datatype="date"/>
	</section>
</record>