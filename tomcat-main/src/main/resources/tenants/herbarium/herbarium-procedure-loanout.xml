<record id="loanout">
	<services-record-path id="herbarium">loansout_herbarium:http://collectionspace.org/services/loanout/local/herbarium,loansout_herbarium</services-record-path>

	<section id="loanoutInformation">
		<field id="majorGroup" section="herbarium" autocomplete="true" ui-type="enum" />

		<repeat id="geographyList" section="herbarium">
			<field id="geography" section="herbarium" />
		</repeat>

		<repeat id="taxonomyList" section="herbarium">
			<field id="taxonomy" section="herbarium" autocomplete="true" />
		</repeat>

		<repeat id="objKindList" section="herbarium">
			<field id="objKind" section="herbarium" autocomplete="true" ui-type="enum" />
		</repeat>

		<field id="shipDate" section="herbarium" datatype="date" />
		<field id="transportMethod" section="herbarium" autocomplete="true" ui-type="enum" />
		<field id="numPackages" section="herbarium" datatype="integer" />
		<field id="acknowledgeDate" section="herbarium" datatype="date" />
	</section>
</record>
