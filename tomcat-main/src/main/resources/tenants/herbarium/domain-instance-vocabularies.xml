<root>
    <include src="extensions/naturalhistory-vocabularies.xml" strip-root="yes" />

    <instance id="vocab-transportmethod">
        <web-url>transportmethod</web-url>
        <title-ref>transportmethod</title-ref>
        <title>Transport Method</title>
        <options>
            <option id="surface">surface</option>
            <option id="hand">hand-carried</option>
            <option id="air">air</option>
        </options>
    </instance>
    <instance id="vocab-hybridparentqualifier">
        <web-url>hybridparentqualifier</web-url>
        <title-ref>hybridparentqualifier</title-ref>
        <title>Hybrid Parent Qualifier</title>
        <options>
            <option id="unknown">unknown</option>
            <option id="cf">cf.</option>
            <option id="fide">fide</option>
            <option id="forsan">forsan</option>
            <option id="group">group</option>
