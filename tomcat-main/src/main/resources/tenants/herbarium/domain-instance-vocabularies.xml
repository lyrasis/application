            <option id="sensu">sensu</option>
            <option id="sensu_lato">sensu lato</option>
            <option id="sensu_stricto">sensu stricto</option>
            <option id="series">series</option>
            <option id="vide">vide</option>
            <option id="intermediate">intermediate</option>
            <option id="question">?Â </option> <!-- display name must have at least 2 characters -->
        </options>
    </instance>
    <instance id="vocab-annotationtype">
        <web-url>annotationtype</web-url>
        <title-ref>annotationtype</title-ref>
