<record id="loanin" is-extension="true">
	<services-record-path id="herbarium">loansin_herbarium:http://collectionspace.org/services/loanin/local/herbarium,loansin_herbarium</services-record-path>

	<section id="loaninInformation">
		<field id="majorGroup" section="herbarium" autocomplete="true" ui-type="enum" />

		<repeat id="geographyList" section="herbarium">
			<field id="geography" section="herbarium" />
		</repeat>

		<repeat id="taxonomyList" section="herbarium">
			<field id="taxonomy" section="herbarium" autocomplete="true" />
		</repeat>

		<repeat id="objKindList" section="herbarium">
			<field id="objKind" section="herbarium" autocomplete="true" ui-type="enum" />
		</repeat>

		<field id="shipDate" section="herbarium" datatype="date" />
		<field id="dueDate" section="herbarium" datatype="date" />

		<field id="transferInDate" section="herbarium" datatype="date" />
		<field id="transferInOrg" section="herbarium" autocomplete="true" />
		<field id="transferInPerson" section="herbarium" autocomplete="true" />

		<field id="numReceived" section="herbarium" datatype="integer" />
		<field id="numReturned" section="herbarium" datatype="integer" />
		<field id="numTransferredOut" section="herbarium" datatype="integer" />
		<field id="balance" section="herbarium" datatype="integer" />
		<field id="discrepancy" section="herbarium" datatype="integer" />
	</section>
</record>
