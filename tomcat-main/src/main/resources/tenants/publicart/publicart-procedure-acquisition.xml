<record id="acquisition" is-extension="true">
	<services-record-path id="publicart">acquisitions_publicart:http://collectionspace.org/services/acquisition/local/publicart,acquisitions_publicart</services-record-path>

	<section id="acquisitionInformation">
		<!-- Local fields -->

		<!-- Replacement for accessionDateGroup, to make a calendar date. -->

		<field id="accessionDate" datatype="date" section="publicart" />

		<!-- Replacement for acquisitionDateGroupList/acquisitionDateGroup, to make a calendar date. -->

		<repeat id="acquisitionDates" section="publicart">
			<field id="acquisitionDate" datatype="date" section="publicart" />
		</repeat>
	</section>
</record>
